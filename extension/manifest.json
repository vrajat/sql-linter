{
  "name": "JSON Pretty Print",
  "version": "1.0",
  "manifest_version": 2,
  "description": "This extension allows you to pretty print JSON.",
  "permissions": [
    "*://*/*",
    "<all_urls>"
  ],
  "browser_action": {},
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}

